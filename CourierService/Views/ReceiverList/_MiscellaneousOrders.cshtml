<script>
    $(document).ready(function () {
        Submiscellaneous();
    });
    $("#submiscellaneous").click(function () {
        Submiscellaneous();
    });
    $("#Expires").click(function () {
        SubExpire();
    });
</script>

<script>
        function Submiscellaneous() {

            $.ajax({
                url: '@Url.Action("GetAllSubMiscellaneousOrders", "ReceiverList")',
                beforeSend: function () {
                    $("#loading-image").show();
                }
		    })
            .done(function (response) {
                $("#MiscellaneousSubPartialView").html(response);
                $("#loading-image").hide();
            })
            .fail(function (XMLHttpRequest, textStatus, errorThrown) {
        	    alert("Sub FAIL");
            });

    }
    function SubExpire() {

            $.ajax({
                url: '@Url.Action("GetAllExpireOrders", "ReceiverList")',
                beforeSend: function () {
                    $("#loading-image").show();
                }
		    })
            .done(function (response) {
                $("#ExpireSubPartialView").html(response);
                $("#loading-image").hide();
            })
            .fail(function (XMLHttpRequest, textStatus, errorThrown) {
        	    alert("Sub FAIL");
            });

        }
</script>