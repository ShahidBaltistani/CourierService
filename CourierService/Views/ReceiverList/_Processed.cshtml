    <script>
        function dateFormat(d) {
            return ((d.getMonth() + 1) + "").padStart(2, "0")
                + "/" + (d.getDate() + "").padStart(2, "0")
                + "/" + d.getFullYear();
        }
        History = function (Id) {

            $.ajax({
                type: "GET",
                url: "/Searching/CheckHistory/" + Id,
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                beforeSend: function () {
                    $("#PdfMergingLoader").show();
                },
                complete: function () {
                    $("#PdfMergingLoader").hide();
                },
                success: function (data) {
                    $('#HistoryBody').html('');
                    $("#HistoryModal").modal("show");
                    $.each(data, function (index, values) {

                        $("#ReceiverName").html(values.Name);
                        tr = "<tr><td>" + values.Device + "</td><td>" + values.CreatedDate + "</td><td>" + values.OrderStatus + "</td><td>" + values.Date + "</td><td>" + values.Remarks + "</td></tr>";
                        $("#HistoryBody").append(tr);
                    });
                },
                error: function (result) {
                    alert('Please Try Again...');
                }
            });
        };
    </script>


