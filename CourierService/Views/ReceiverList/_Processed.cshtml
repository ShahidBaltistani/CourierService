<script>
    //pass pickupdate and id's in array to creating order methode
    $("#oneperPdfs").click(function () {

        var List = [];




        $("input[name='checkboxselected']:checked").each(function (id) {

            var closestTr = $(this).closest('tr').attr('id');
            List.push(closestTr);

        });
        var data = { Id: List };


        if (jQuery.isEmptyObject(data.Id)) {
            alert("Please select at least one order......");
            return false;
        }


        //var json = JSON.stringify(data0);
        $.ajax({
            type: "POST",
            url: "/ReceiverList/OnePerPdfDownload",
            data: data,
            beforeSend: function () {
                $("#PdfMergingLoader").show();
            },
            complete: function () {
                $("#PdfMergingLoader").hide();
            }
		})
        .done(function (res) {


            window.location.href = "@Url.RouteUrl(new
                        {  Action = "Download"})/?file=" + res;
            // location.reload();
        })
        .fail(function (XMLHttpRequest, textStatus, errorThrown) {
                swal("Fail!");
        });
    });
</script>

