                                    </span>
                                </label>
                            </div>
                        </td>
                        <td style="width:100px !important;">@item.Receiver.Name</td>
                        @*<td style="width:150px !important;">@item.Receiver.Address_Line2</td>*@
                        <td style="width:150px !important;">@(item.Receiver.Address_Line2==null ? item.Receiver.Address_Line1 + item.Receiver.Suburb+ "," + item.Receiver.State_Name + " " + item.Receiver.Postcode: item.Receiver.Address_Line2)</td>


                        @if (item.Customer_Reference == null)
                        {
                            <td style="width:100px !important;">
                                <form id="add_CustomerRefrenceprocessed">
                                    <input type="text" hidden class="form-control orderIdProcessed" value="@item.Id" name="Id" id="cref-id" style=" max-width: 560px;display: none;">

